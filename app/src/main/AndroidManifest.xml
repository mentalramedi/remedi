<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.remedi">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".testCategories.QuestionnairesActivity" />
        <activity android:name=".testCategories.VideosActivity" />
        <activity android:name=".Devices.AddDeviceActivity" />
        <activity android:name=".RegisteredMemberDetailsActivity" />
        <activity android:name=".questions.QuestionActivity"/>
        <activity android:name=".questions.AnswersActivity"/>
        <activity
            android:name=".Devices.DevicesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TestsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".progress.ProgressActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".members.AddmemberActivity"
            android:parentActivityName=".members.MembersActivity" />
        <activity
            android:name=".members.MembersActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ProfileActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".signup_in_setup.SetupActivity" />
        <activity android:name=".signup_in_setup.LoginActivity" />
        <activity android:name=".signup_in_setup.Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
    </application>

</manifest>